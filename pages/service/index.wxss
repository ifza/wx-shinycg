/* index.wxss */

@import '../common/global.wxss';

.form{ padding:15px; }
.form .form-group{margin:8px 0; }
.form .form-group .form-control{
  border:1px solid #ddd;
  padding:4px;
  height: auto;
  box-sizing: border-box;
  border-radius:4px;
  width:auto;
}
.page-xuqiu .application-item{

  background-image: url(data:image/jpeg;base64,);
    display: block;
  height: 60px;
  background-repeat: no-repeat;
  background-position: left center;
  line-height: 60px;
  font-size: 40rpx;
  color:#fff;
  font-weight: bold;
  text-align: right;
  margin:20px;  
  
  background-color: #88a7ec;
  
  box-shadow: 10px 10px 10px #ddd;
  padding:10px;
}