@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
		url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}



.tabs{ display: flex;
  height:40px;
  flex-direction: row;
}
.tabs .tab{
  border-bottom: 3px solid #eee;
  font-size: 16px;
  height: 40px;
  line-height: 37px;
  box-sizing:border-box;
  text-align: center;
  flex-grow:1;
  color:#333;

}
.tabs .tab.active{
  color:#88a7ec;
  border-color:#88a7ec;
}



.Footer{
  background-color: #88a7ec;
  box-sizing: border-box;
  padding:15px;
}
.Footer .Address text{ color:#fff;}
.Footer-Logo{
  background-image: url(https://www.shinycg.com/Public/Home/img/footer-logo.png);
  height: 80px;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: contain;
}
.Footer .Copy{ color:#fff;}

.Product{ border:1px solid #88a7ec; margin: 10px; }
.Product .Pic{ height: 220px;  background-size:cover;  background-position: center center; background-repeat:no-repeat;}
.Product .Title{ background-color:#88a7ec;  font-size: 15px; color:#fff; text-align: center; height: 30px; line-height: 30px;}



.content,.p,.h1,.h2,.h3,.h4,.h5,.h5{display: block; box-sizing:border-box;}
.h1,.h2,.h3,.h4,.h5,.h5{font-weight: bold; line-height: 200%; text-align: center;}
.p{font-size:14px;line-height: 200%;}
.h1{font-size:24px;}
.h2{font-size:22px;}
.h3{font-size:20px;}
.h4{font-size:18px;}
.h5{font-size:14px;}
.h6{font-size:12px;}


.text-center{ text-align: center;}

.p4{ padding:4px;}
.p6{ padding:6px;}
.p8{ padding:8px;}
.p10{ padding:10px;}
.p12{ padding:12px;}


.ts-30{font-size: 90px !important; line-height: 90px; color:#88a7ec;}

.hr{height: 1px; background-color: #ddd; margin:10px 0; display: block;}


